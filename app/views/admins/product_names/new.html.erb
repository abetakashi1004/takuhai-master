
<div class="container">
	 <div class="row">
		<div class="col">
			<h1>品名追加登録</h1>
			<%= form_for(@product_name, url: admins_product_names_path) do |f| %>
			<%= f.label :品名 %><br>
			<%= f.text_field :product_name, placeholder: "品名" %>
			<div><%= f.submit '品名を登録', class: 'btn btn-primary submit_botton' %></div>
			<% end %>
		</div>
	</div>
</div>